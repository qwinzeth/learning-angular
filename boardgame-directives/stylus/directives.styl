#divBoard
	position relative

.tile
	height 49px
	width 49px
	text-align center

tile, .player
	position absolute
	
.tile-move-again
	background-color: #00FF00

.tile-start
	background-color: #00BB00
	
.tile-normal
	background-color: #FFFF00
	
.player
	z-index 1

.dice-container
	margin-bottom 2px
	
.die
	display inline-block
	border 1px solid black
	width 20px
	height 20px
	text-align center
	vertical-align middle
	cursor pointer